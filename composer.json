    "primitives",
    "objective php"
  ],
  "homepage": "https://github.com/objective-php/components",
  "license": "GPL-3.0",
  "authors": [
    {
      "name": "Gauthier 'Rusty' Delamarre",
      "email": "gauthier@objective-php.org",
      "role": "lead"
    },
